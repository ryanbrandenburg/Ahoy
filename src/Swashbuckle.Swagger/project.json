{
  "version": "6.0.0-beta9",
  "description": "Core components for Swashbuckle - a Swagger toolchain for AspNet Core",
  "authors": [ "Richard Morris" ],

  "packOptions": {
    "tags": [ "Swagger Documentation Discovery Help WebApi AspNet AspNetCore" ],
    "projectUrl": "https://github.com/domaindrivendev/Swashbuckle",
    "licenseUrl": "https://github.com/domaindrivendev/Swashbuckle/LICENSE",
    "summary": "Provides a Swagger object model and middleware that can be used to expose Swagger descriptions for your API"
  },

  "dependencies": {
    "Microsoft.AspNetCore.Mvc.Core": "1.0.0-*",
    "Microsoft.AspNetCore.Mvc.Formatters.Json": "1.0.0-*"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      }
    }
  }
}